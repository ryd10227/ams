{
  "name": "bizMOB4Vue",
  "version": "0.0.1",
  "private": true,
  "description": "bizMOB4 Ionic+Vue project",
  "scripts": {
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\"",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "//": "## all ##",
    "serve-dev": "vue-cli-service serve --mode development",
    "serve-stag": "vue-cli-service serve --mode staging",
    "serve-prod": "vue-cli-service serve --mode production --port 3000",
    "build-dev": "vue-cli-service build --mode development",
    "build-stag": "vue-cli-service build --mode staging",
    "build-prod": "vue-cli-service build --mode production",
    "serve-dev:en": "cross-env COUNTRY=en vue-cli-service serve --mode development",
    "serve-stag:en": "cross-env COUNTRY=en vue-cli-service serve --mode staging",
    "serve-prod:en": "cross-env COUNTRY=en vue-cli-service serve --mode production --port 3000",
    "build-dev:en": "cross-env COUNTRY=en vue-cli-service build --mode development",
    "build-stag:en": "cross-env COUNTRY=en vue-cli-service build --mode staging",
    "build-prod:en": "cross-env COUNTRY=en vue-cli-service build --mode production",
    "build-dev:all": "npm run build-dev && npm run build-dev:en",
    "build-stag:all": "npm run build-stag && npm run build-stag:en",
    "build-prod:all": "npm run build-prod && npm run build-prod:en"
  },
  "dependencies": {
    "@ionic/vue": "^6.0.0",
    "@ionic/vue-router": "^6.0.0",
    "@types/crypto-js": "^4.2.1",
    "@vue/preload-webpack-plugin": "^2.0.0",
    "axios": "^1.6.1",
    "chart.js": "^4.4.0",
    "core-js": "^3.6.5",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "esm": "^3.2.25",
    "moment": "^2.29.3",
    "sass": "^1.52.1",
    "sass-loader": "^12.6.0",
    "swiper": "^11.0.4",
    "vue": "^3.3.8",
    "vue-chartjs": "^5.2.0",
    "vue-i18n": "^9.1.0",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^4.0.12",
    "vuetify": "^3.4.0",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^3.0.0",
    "@types/jest": "^27.0.2",
    "@types/vue": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@vue/cli-plugin-babel": "~5.0.0-rc.1",
    "@vue/cli-plugin-e2e-cypress": "~5.0.0-rc.1",
    "@vue/cli-plugin-eslint": "~5.0.0-rc.1",
    "@vue/cli-plugin-router": "~5.0.0-rc.1",
    "@vue/cli-plugin-typescript": "~5.0.0-rc.1",
    "@vue/cli-plugin-unit-jest": "~5.0.0-rc.1",
    "@vue/cli-service": "5.0.4",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.0-rc.16",
    "@vue/vue3-jest": "^27.0.0-alpha.3",
    "babel-jest": "^27.3.1",
    "cross-env": "^7.0.3",
    "cypress": "^8.7.0",
    "eslint": "^8.4.1",
    "eslint-plugin-vue": "^8.2.0",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "typescript": "^4.3.5",
    "vue-cli-plugin-i18n": "~2.3.1"
  }
}
